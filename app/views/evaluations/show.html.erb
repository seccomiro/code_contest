<% content_for :title do %>
  Details of Evaluation
<% end %>

<div class="page-header">
  <h1>Details of Evaluation</h1>
  <div>
    <%= link_to edit_evaluation_path(@evaluation), class: 'btn btn-success' do %>
      <%= icon 'fas', 'edit' %>
      Edit
    <% end %>
    <%= link_to evaluations_path, class: 'btn btn-outline-secondary' do %>
      <%= icon 'fas', 'bars' %>
      Back
    <% end %>
  </div>
</div>

<dl class="dl-horizontal">
  <dt>Submission:</dt>
  <dd><%= @evaluation.submission %></dd>

  <dt>Judge:</dt>
  <dd><%= @evaluation.judge %></dd>

  <dt>Result:</dt>
  <dd><%= @evaluation.result %></dd>

</dl>
